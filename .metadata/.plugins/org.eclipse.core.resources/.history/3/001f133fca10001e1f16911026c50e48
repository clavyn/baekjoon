package baekjoon;

import java.util.Arrays;

public class Main {

	static int[] solution (String s) {
		int[] answer = new int[2];
		int zeroCnt = 0;
		int twoCnt = 0;
		int length = 0;
		int num2=0;
		String tmp="";
		String result="";
		for(int i = 0 ; i < s.length(); i++) {
			if(s.charAt(i)=='1') {
				tmp+='1';
			}else {
				zeroCnt++;
			}
		}

		twoCnt++;

		num2=Integer.parseInt(tmp.length()+"",10);
		System.out.println("num2 : "+num2);
		
		answer[0]=twoCnt;
		answer[1]=zeroCnt;
		return answer;
		
	}
	

	public static void main(String[] args) {
		System.out.println(Arrays.toString(solution("110010101001")));
	}
}
