package baekjoon;

import java.util.Arrays;
import java.util.Stack;

public class Main {

	static int solution(int n) {
		int f1 = 0;
		int f2 = 1;
		int cnt=0;
		
		while(f1+f2<n) {
			f1=f2;
			f2=f1+f2;	
		}
			System.out.println(f1+f2);	
			
			
		
		
		return 1%1234567;
	}

	public static void main(String[] args) {

		System.out.println(solution(5));

	}
}
