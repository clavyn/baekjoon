package baekjoon;

import java.util.Arrays;

public class Main {

	static String solution(String s) {
		String answer = "";
		int len = s.length();
		if(len%2==0) {
			answer=s.charAt(len/2)+""+s.charAt(len/2+1)+"";
		}else {
			answer = s.charAt((len-1)/2)+"";
		}
		
		
		return answer;
	}

	public static void main(String[] args) {
		System.out.println(solution("qwer"));

	}
}
