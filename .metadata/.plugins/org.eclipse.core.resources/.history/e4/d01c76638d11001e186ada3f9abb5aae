package baekjoon;

import java.util.Stack;

public class Main {

	static int solution(int n) {
		int answer = 0;
		String bin = "";
		bin = Integer.toBinaryString(n);
		System.out.println(bin);
				
		
	
		return answer;
		

	}

	public static void main(String[] args) {
		System.out.println(solution(78));//83
		System.out.println(solution(15));//23

	}
}
