package baekjoon;

import java.util.Arrays;
import java.util.Stack;

public class Main {

	static int solution(int n) {
		int f1 = 0;
		int f2 = 1;
		int cnt=0;
		
		while(cnt==n) {
			f1=f2;
			f2=f1+f2;
			cnt++;			
		}
				
			
			
		
		
		System.out.println(cnt);
		return cnt%1234567;
	}

	public static void main(String[] args) {

		System.out.println(solution(5));

	}
}
