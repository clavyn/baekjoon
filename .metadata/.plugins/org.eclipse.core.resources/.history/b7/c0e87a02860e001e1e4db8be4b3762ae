package baekjoon;

import java.util.Arrays;

public class Main {

	static int solution(String s) {
		
		int answer = 0 ;
		try {
			answer=Integer.parseInt(s);
		}catch(Exception e) {
			s=s.substring(1, s.length());
			answer=-Integer.parseInt(s);
		}
		
		return answer;
		
		
	}

	public static void main(String[] args) {
		System.out.println(solution("-1234"));

	}
}
