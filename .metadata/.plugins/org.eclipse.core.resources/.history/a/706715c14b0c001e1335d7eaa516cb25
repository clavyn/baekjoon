package baekjoon;

import java.util.Arrays;

public class Main {

	static long solution(long n) {
		double tmp = Math.sqrt(n);
		
		long answer=0;
		return answer = (long) ((tmp % 1 == 0) ? (tmp + 1) * (tmp + 1) : -1);
	}

	public static void main(String[] args) {

		System.out.println(solution(121));

	}
}
