package baekjoon;

import java.util.Arrays;
import java.util.Stack;

public class Main {

	static int solution(int n) {
		int answer = 0;
		int[] e = new int [n];
		int tmp=0;

		for(int i = 1 ; i <= n ; i++) {
				tmp = i;				
			for(int j = i+1 ; j<=n ;j++) {
				tmp +=j;
				if(tmp==15) {
					answer++;
					break;
				}
			}
		}
		
		return answer+1;
	}

	public static void main(String[] args) {

		System.out.println(solution(15));

	}
}
