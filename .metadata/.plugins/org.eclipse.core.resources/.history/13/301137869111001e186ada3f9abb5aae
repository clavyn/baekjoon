package baekjoon;

import java.util.Arrays;
import java.util.Stack;

public class Main {

	static int solution(int []d, int budget) {
		int answer = 0;
		int hap = 0;
		Arrays.sort(d);
		for(int i=0;i<d.length;i++) {
			hap+=d[i];
			if(budget>=hap) answer=i;
		}

		return answer;

	}

	public static void main(String[] args) {
		int [] d = {2,2,3,3};
		
		System.out.println(solution(d,10));//3
	
	}
}
