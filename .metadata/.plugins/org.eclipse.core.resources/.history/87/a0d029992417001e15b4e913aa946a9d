package baekjoon;

import java.util.Arrays;

public class Main {

	static String solution(int a, int b) {
		String answer = "";
		int[] arr = {31,29,31,30,31,30,31,31,30,31,30,31};
		int tmp=b;
		for (int i = 0; i <5; i++) {
			tmp+=arr[i];
		}
		System.out.println(tmp%7);
		return tmp%7+"";

	}

	public static void main(String[] args) {
		System.out.println(solution(5, 24));// "TUE"
	}
}
