package baekjoon;

import java.util.ArrayList;
import java.util.Arrays;

public class Main {

	static int[] solution(int[] arr, int divisor) {
		int[] answer = new int[arr.length];
		int [] re;
		int j=0;
		for(int i = 0; i<arr.length;i++) {
			if(arr[i]%divisor==0) {
				answer[j]=arr[i];
				j++;
			}
		}
		if(j==0) {
			re=new int[1];
			re[0]=-1;
			return re;
		}
		re = new int[j];
		for(int i = 0 ; i < j ; i++) {
			re[i]=answer[i];			
		}
		int tmp = 0;
		for(int i = 0 ; i < j ; i++) {
			for(int x= 0 ; x<i-1; x++) {
				if(re[x]<re[x+1]) {
					tmp=re[x+1];
					re[x+1]=re[x];
					re[x]=tmp;
				}
			}
		}
		return re;
	}

	public static void main(String[] args) {
		int[] arr = {2,36,1,3};
		System.out.println(Arrays.toString(solution(arr,1)));

	}
}
