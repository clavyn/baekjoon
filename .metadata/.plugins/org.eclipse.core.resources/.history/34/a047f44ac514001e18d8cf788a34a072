package baekjoon;


public class Main {

	static String solution(int[] food) {
		String answer = "";
		int cnt = 0 ;
		for(int i = 1 ; i < food.length ; i++) {
			cnt=0;
			while(cnt==food[i]/2) {
				System.out.println("mm");
				answer+=i;
				cnt++;				
			}
		}
		
		return answer;

	}

	public static void main(String[] args) {
		int [] food = {1,3,4,6};
		//1223330333221
		int [] foods = {1,7,1,2};
		//111303111
		System.out.println(solution(food));// 0
		System.out.println(solution(foods));// 0

	}
}
