package baekjoon;

import java.util.Stack;

public class Main {

	static String solution(int n) {
		String answer ="";
		int mok=0;//ыкл

		Stack<Integer> stk = new Stack<>();
		
		while(n!=1) {
			System.out.println(n);
			n/=2;
			stk.push(n%2);
		}
		for(int i = 0 ;i< 4 ; i++) {			
			answer+=stk.pop();
		}
		
		
		return answer;
		

	}

	public static void main(String[] args) {
		System.out.println(solution(20));

	}
}
