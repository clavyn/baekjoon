package baekjoon;

import java.util.Arrays;


public class Main {

	static String solution(String s) {
		int answer = 0;
		for(int i = 0 ; i < s.length() ; i++) {
			s=s.replace("one", "1");
		}
		
		
		return s;
	}

	public static void main(String[] args) {


		System.out.println(solution("one4seveneight"));//1478

	}
}
