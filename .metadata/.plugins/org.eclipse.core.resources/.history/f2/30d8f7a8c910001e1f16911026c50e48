package baekjoon;

import java.util.Arrays;

public class Main {

	static int[] solution (String s) {
		int[] answer = new int[2];
		int zeroCnt = 0;
		int length = 0;
		String tmp="";
		for(int i = 0 ; i < s.length(); i++) {
			if(s.charAt(i)=='1') {
				tmp+='1';
			}else {
				zeroCnt++;
			}
		}

		for(int i = tmp.length() ; i>0;i--) {
			if(i==2)tmp+=1;
			tmp+=i%2;
			
		}
		
		answer[1]=zeroCnt;
		return answer;
		
	}
	

	public static void main(String[] args) {
		System.out.println(Arrays.toString(solution("1110")));
	}
}
