package baekjoon;

import java.util.ArrayList;
import java.util.Arrays;

public class Main {

	static String solution (String s) {
		String answer ="";
		String [] tmp = s.split(" ");
		System.out.println(Arrays.toString(tmp));
		return answer;
		
	}
	

	public static void main(String[] args) {
		System.out.println(solution("try hello world"));
	}
}
