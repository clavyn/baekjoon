package baekjoon;

import java.util.Arrays;

public class Main {

	static String solution(String s) {
		String answer = "";
		char[] arr = s.toCharArray();
		char tmp;
		int len = s.length();
		for(int i = 0 ; i < len ; i++) {
			for(int j = 0 ; i < len-1; j++) {
				if(arr[j]<arr[j+1]) {
					tmp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=tmp;
				}
			}
		}
		for(int i = 0 ;i< arr.length ; i++) {
			answer+=arr[i];
		}
		
		
		return answer;
	}

	public static void main(String[] args) {
		System.out.println(solution("Zbcdefg"));

	}
}
